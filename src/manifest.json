{
  "package": "com.ykrc17.githubtrending.quickapp",
  "name": "GithubTrending",
  "versionName": "1.0.0",
  "versionCode": 1,
  "icon": "/Common/logo.png",
  "minPlatformVersion": 1000,
  "features": [
    {
      "name": "system.router"
    },
    {
      "name": "system.fetch"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "logLevel": "log"
  },
  "router": {
    "entry": "Home",
    "pages": {
      "Home": {
        "component": "home",
        "path": "/"
      },
      "Repo": {
        "component": "repo",
        "path": "/repo"
      }
    }
  },
  "display": {
    "backgroundColor": "#fbf9fe",
    "titleBar": true,
    "titleBarBackgroundColor": "#24292e",
    "titleBarTextColor": "#ffffff",
    "menu": true
  }
}